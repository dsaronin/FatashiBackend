{
  "filename": "data/kauli_data.txt",
  "kamusiType": "methali",
  "kamusiLang": "kiswahili",
  "fieldDelimiters": "$",
  "wrapKeyHead":  "^[^\\t]*",
  "wrapKeyTail":  "[^\\t]*\\t",
  "wrapDefHead":  "^[^\\t]+\\t[^\\t]*",
  "wrapDefTail":  "[^\\t]*$",
  "wrapUsgHead":  "^[^\\t]+\\t[^\\t]*",
  "wrapUsgTail":  "[^\\t]*$",
  "hashtagOn": true,
  "hashtagWrapPattern": "(#)",
  "percentOn": false,
  "ampersandOn": true,
  "atsignOn": true,
  "atsignWrapPattern": "{@}"
}
